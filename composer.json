{
	"name": "chillerlan/php-authenticator",
	"description": "A generator for counter- and time based authentication codes. PHP 7+",
	"homepage": "https://github.com/codemasher/php-authenticator",
	"license": "MIT",
	"type": "library",
	"keywords": [
		"authenticator", "totp", "hotp", "2fa", "rfc4226", "rfc6238"
	],
	"authors": [
		{
			"name": "Smiley",
			"email": "smiley@chillerlan.net"
		}
	],
	"require": {
		"php": ">=7.0.3"
	},
	"require-dev": {
		"phpunit/phpunit": "6.5.*"
	},
	"suggest": {
		"chillerlan/php-qrcode": ">=2.0.0"
	},
	"autoload": {
		"psr-4": {
			"chillerlan\\Authenticator\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"chillerlan\\AuthenticatorTest\\": "tests/"
		}
	}
}
